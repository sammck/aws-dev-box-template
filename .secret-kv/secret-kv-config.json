{
  "cfg_class": "secret_kv.config.sql_store.SqlKvStoreConfig",
  "data": {
    "file_name": "${config_dir}/kv.db",
    "passphrase_cfg": {
      "cfg_class": "secret_kv.config.keyring_passphrase.KeyringPassphraseConfig",
      "data": {
        "default_passphrase_cfg": {
          "cfg_class": "secret_kv.config.keyring_passphrase.KeyringPassphraseConfig",
          "data": {
            "key": "default-db-passphrase",
            "service": "python/secret-kv"
          }
        },
        "key": "fhash/${config_file_hash}/db-passphrase",
        "service": "python/secret-kv"
      }
    }
  },
  "version": "1.3.0"
}
